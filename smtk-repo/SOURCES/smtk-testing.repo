[kaos-testing]
name=ESSENTIAL KAOS Testing (Unstable) Repo for RHEL6/CentOS6 - $basearch
baseurl=http://testing.yum.kaos.st/$basearch
mirrorlist=file:///usr/local/share/kaos-repo/testing/$basearch.mirrors
failovermethod=priority
priority=1
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ESSENTIALKAOS


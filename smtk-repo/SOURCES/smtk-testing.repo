[smtk-testing]
name=SIMTECH Testing (Unstable) Repo for RHEL/CentOS - $basearch
baseurl=http://testing.yum.smtk.us/$releasever/$basearch
mirrorlist=file:///usr/local/share/smtk-repo/testing/$basearch.mirrors
failovermethod=priority
priority=1
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-SIMTECH


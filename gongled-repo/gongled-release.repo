[gongled-release]
name=GONGLED Release (Stable) Repo for RHEL/CentOS
baseurl=https://release.yum.gongled.me/$releasever/$basearch/
enabled=1
failovermethod=priority
priority=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-GONGLED

[gongled-release-src]
name=GONGLED Release (Stable) Repo for RHEL/CentOS
baseurl=https://release.yum.gongled.me/$releasever/SRPMS/
enabled=0
failovermethod=priority
priority=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-GONGLED

[gongled-release-i386]

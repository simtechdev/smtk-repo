priority=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-GONGLED

[gongled-release-x64]
name=GONGLED Release (Stable) Repo for RHEL/CentOS
baseurl=https://release.yum.gongled.me/$releasever/x86_64/
